                        <div class="widget-content widget-content-area br-6">
                            <table id="invoice-list" class="table table-hover" style="width:100%">
                                <thead>
                                    <tr> 
                                        
                                        <th>Code Id</th>
                                        <th>Nombre Paciente</th>
                                        <th>Nombre Doctor</th>
                                        <th>Date</th>
                                        <th>Hora</th>
                                        <th>Estado</th>
                                        <th>T/ Atencion</th>
                                    </tr>
                                </thead>
                                <tbody>
                                     <% 
										String error = (String)request.getAttribute("mensaje");
									%>
                                	<% 
										List<Cita> listCita = (List<Cita>)request.getAttribute("lista");
					
											if(listCita != null){
												for(Cita item: listCita){
									%>
                                    <tr> 
                                        
                                        <td><a href="apps_invoice-preview.html"><span class="inv-number">#098424</span></a></td>
                                        <td>
                                            <div class="d-flex">
                                                <p class="align-self-center mb-0 user-name"><%=item.id_paciente %></p>
                                            </div>
                                        </td>
                                        <td><span class="align-self-center mb-0 user-name"><%=item.getId_personal() %></span></td>
                                        <td><span class="inv-date"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-calendar"><rect x="3" y="4" width="18" height="18" rx="2" ry="2"></rect><line x1="16" y1="2" x2="16" y2="6"></line><line x1="8" y1="2" x2="8" y2="6"></line><line x1="3" y1="10" x2="21" y2="10"></line></svg> <%=item.getFecha() %></span></td>
                                        <td><span class="inv-amount"><%=item.getHora() %></span></td>
                                        <td><span class="badge badge-success inv-status"><%=item.getEstado() %> </span></td>
                                        <td><span class="badge badge-success inv-status"><%=item.getTipo_atencion() %> </span></td>
                                        <td>
                                                <a href="#editEmployeeModal" class="edit" data-toggle="modal"><i class="material-icons" data-toggle="tooltip" title="Edit">&#xE254;</i></a>
                                                <a href="#deleteEmployeeModal" class="delete" data-toggle="modal"><i class="material-icons" data-toggle="tooltip" title="Delete">&#xE872;</i></a>
                                        </td>
                                    </tr>
					<%
